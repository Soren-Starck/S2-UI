{
  "name": "s2-ui",
  "version": "1.0.0",
  "description": "A streamlined UI component library with easy theming and component management",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "s2-ui": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest",
    "prepare": "npm run build",
    "cli": "node ./dist/cli/index.js",
    "test:page": "vite --config vite.config.js --host",
    "test:build": "vite build --config vite.config.js",
    "start": "npm run test:page",
    "watch": "nodemon --watch src --ext ts,tsx --exec \"npm run build\""
  },
  "keywords": [
    "ui",
    "components",
    "tailwind",
    "react"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "execa": "^5.1.1",
    "framer-motion": ">=10.0.0",
    "fs-extra": "^11.1.1",
    "inquirer": "^8.2.5"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/inquirer": "^8.2.6",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "eslint": "^8.42.0",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^29.5.0",
    "nodemon": "^3.1.9",
    "postcss": "^8.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3",
    "vite": "^4.5.11"
  },
  "peerDependencies": {
    "framer-motion": ">=10.0.0",
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0",
    "tailwindcss": ">=3.0.0"
  }
}
